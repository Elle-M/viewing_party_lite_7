<%= render "welcome/welcome_page_nav" %>

<%= button_to "Discover Top Rated Movies", user_movies_path(@user), method: :get %>

<%= form_with url: user_movies_path(@user), method: :post, local: true do |f| %>
	<%= f.label :movie_title, "Movie Title" %>
	<%= f.text_field :movie_title %> 
  <%= f.submit "Search" %>
<% end %>