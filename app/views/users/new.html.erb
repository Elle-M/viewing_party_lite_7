<%= form_with model: @user, method: :post, local: true do |f| %>
	<%= f.label :name, "Name" %>
	<%= f.text_field :name %>
	<%= f.label :email, "Email" %>
	<%= f.email_field :email %>
  <%= f.label :user_password, "Password" %>
  <%= f.password_field :password %>
  <%= f.label :user_password_confirmation, "Password Confirmation" %>
  <%= f.password_field :password_confirmation %>
	<%= f.submit "Register", class: "btn btn-success"%>
<% end %>